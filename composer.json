{
	"name": "forxer/gravatar",
	"description": "A library providing easy gravatar integration.",
	"homepage": "https://github.com/forxer/gravatar",
	"keywords": [
		"gravatar",
		"php"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Vincent Garnier",
			"email": "forxer@gmail.org"
		}
	],
	"support": {
		"issues": "https://github.com/forxer/gravatar/issues",
		"email": "forxer@gmail.com"
	},
	"require": {
		"php": "^8.2"
	},
    "require-dev": {
        "laravel/pint": "^1.21.2",
        "rector/rector": "^2.0.10"
    },
	"autoload": {
		"psr-4": {
			"Gravatar\\": "src"
		}
	},
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
